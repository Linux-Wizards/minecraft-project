<?xml version="1.0"?> <!-- This file will automatically regenerate on each server start! If you don't want this behavior, the code needs to be changed -->
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "https://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="io.github.darkognu.simplenotepad.Note" table="{{ table_name }}">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="title" type="java.lang.String">
            <column name="title" />
        </property>
        <property name="message" type="java.lang.String">
            <column name="message" />
        </property>
        <property name="playerId" type="java.util.UUID">
            <column name="player_id" />
        </property>
    </class>
</hibernate-mapping>
